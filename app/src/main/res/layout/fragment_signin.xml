<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".accountAdmin.fragment.SignInFragment"
    >

    <TextView
        android:id="@+id/LogIn_tv_LogIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="로그인"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.163" />

    <Button
        android:id="@+id/LogIn_bt_LogIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:background="@android:color/transparent"
        android:text="회원가입"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/LogIn_tv_password"
        app:layout_constraintTop_toBottomOf="@+id/logIn_bt_logIn" />

    <EditText
        android:id="@+id/LogIn_et_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toEndOf="@+id/LogIn_tv_tel"
        app:layout_constraintTop_toBottomOf="@+id/LogIn_tv_LogIn" />

    <EditText
        android:id="@+id/LogIn_et_passwrod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/LogIn_et_tel"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/LogIn_et_tel"
        app:layout_constraintTop_toBottomOf="@+id/LogIn_et_tel" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="72dp"
        android:text="/"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/LogIn_bt_LogIn"
        app:layout_constraintTop_toBottomOf="@+id/LogIn_et_passwrod" />

    <TextView
        android:id="@+id/LogIn_tv_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="비밀번호"
        app:layout_constraintStart_toStartOf="@+id/LogIn_tv_tel"
        app:layout_constraintTop_toBottomOf="@+id/LogIn_tv_tel" />

    <TextView
        android:id="@+id/LogIn_tv_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="60dp"
        android:text="전화번호"
        app:layout_constraintEnd_toStartOf="@+id/LogIn_tv_LogIn"
        app:layout_constraintTop_toBottomOf="@+id/LogIn_tv_LogIn" />

    <Button
        android:id="@+id/logIn_bt_findPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="14dp"
        android:background="@android:color/transparent"
        android:text="비밀번호 찾기"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/LogIn_et_passwrod"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/logIn_bt_logIn" />

    <Button
        android:id="@+id/logIn_bt_logIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="로그인"
        app:layout_constraintEnd_toEndOf="@+id/LogIn_et_passwrod"
        app:layout_constraintStart_toStartOf="@+id/LogIn_et_passwrod"
        app:layout_constraintTop_toBottomOf="@+id/LogIn_et_passwrod" />

</androidx.constraintlayout.widget.ConstraintLayout>